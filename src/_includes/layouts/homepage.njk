<script src="/assets/js/swiper.min.js"></script>
<script>
    // speakers slider
    function initSwiper(refs){
        swiper = new Swiper(refs.container, {
            loop: false,
            slidesPerView: 2,
            spaceBetween: 40,
        
            breakpoints: {
                1024: {
                slidesPerView: 4,
                spaceBetween: 60,
                },
            },
        });
        swiper.on('transitionEnd', function() {
            index=swiper.realIndex;
        });

        return swiper;
    }

    //intro animation
    var duration = 100;
    
    (function() {
        var wrapper = document.getElementById("intro-wrapper");
        var bandsWrapper = document.getElementById("bands-wrapper");
        var introText = document.getElementById("intro-text");
        var bandOne = document.getElementById("band-one");
        var bandTwo = document.getElementById("band-two");
        var bandThree = document.getElementById("band-three");
        var height = window.innerHeight + duration;
        wrapper.style.height = height + "px";
        introText.style.top = (window.innerHeight/2 - introText.offsetHeight/2 ) + "px";
        bandsWrapper.style.top = (window.innerHeight/2 - bandsWrapper.offsetHeight/2 ) + "px";

        window.addEventListener('resize', function() {
            // prevent iOS safari bug
            if(Math.abs(window.innerHeight + duration - height) > 120){
                height = window.innerHeight + duration;
                wrapper.style.height = height + "px";
                introText.style.top = (window.innerHeight/2 - introText.offsetHeight/2 ) + "px";
                bandsWrapper.style.top = (window.innerHeight/2 - bandsWrapper.offsetHeight/2  ) + "px";
            }
        });
        window.addEventListener('scroll', function() {
            var progress =  window.scrollY  / (wrapper.offsetHeight);
            bandOne.style.left =  progress * window.innerWidth - 10 +"px";
            bandTwo.style.left =  "-" + progress * window.innerWidth + 10 + "px";
            bandThree.style.left =  progress * window.innerWidth - 10 + "px";
        });
    })();
</script>