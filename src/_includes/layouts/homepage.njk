<script>
    // speakers slider
    function initSwiper(refs){
        swiper = new Swiper(refs.container, {
            loop: false,
            slidesPerView: 1.4,
            spaceBetween: 40,
        
            breakpoints: {
                1024: {
                    slidesPerView: 3.5,
                    spaceBetween: 60,
                },
                1400: {
                    slidesPerView: 4.5,
                    spaceBetween: 60,
                },
            },
        });
        swiper.on('transitionEnd', function() {
            index=swiper.realIndex;
        });
        return swiper;
    }

    var top_writing = document.querySelector('#top-writing');
    var bottom_writing = document.querySelector('#bottom-writing');
    var speed = 1;
    // on page scroll event
    window.onscroll = function() {
        top_writing.style.transform = "translateX(calc(-200vw + " + (speed*window.scrollY) + "px))";
        bottom_writing.style.transform = "translateX(calc(-200vw - " + (speed*window.scrollY) + "px))";
    }

</script>